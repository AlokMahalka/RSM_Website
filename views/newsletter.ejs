<% layout('layouts/boilerplate')%>

  <main id="main">
    <section class="breadcrumbs">
      <div class="container">
        <div class="d-flex justify-content-between align-items-center">
          <h2>NEWSLETTER</h2>
          <ol>
            <li><a href="/">Home</a></li>
            <li>Newsletter</li>
          </ol>
        </div>
      </div> 
    </section>
    <section class="newsletter spade">
      <div class="container">
        <div class="section-title">
          <h2>OUR NEWSLETTERS</h2>
          <form action="/subscribe" method="POST">
            <input type="text" name="email" placeholder="Enter your email to subscribe">&nbsp;&nbsp;&nbsp;
            <button type="submit"><i class="icofont-location-arrow"></i></button>
          </form>
        </div>
        <div class="row newsletter-container" data-aos="fade-up" data-aos-easing="ease-in-out" data-aos-duration="500">
          <div class="col-lg-4 col-md-6">
            <div class="newsletter-item">
              <img src="img/NewsLetter1.png" class="img-fluid">
              <div class="newsletter-info">
                <h3><a href="https://drive.google.com/file/d/1cuCF0SwX3tXooIcFp46fyIe0EZrl_i2H/view?usp=drivesdk">The SARS CoV- 2 Virus</a></h3>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section> 
      
  </main>